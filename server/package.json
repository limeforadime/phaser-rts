{
  "name": "phaser-rts-server",
  "version": "1.0.0",
  "description": "Server side of phaser-rts-game",
  "main": "./src/server.ts",
  "scripts": {
    "tsc": "tsc",
    "runOnly": "ts-node ./src/utils/server.ts",
    "watch": "ts-node-dev --inspect --respawn --transpileOnly ./src/utils/server.ts",
    "temp": "ts-node ./src/utils/server.ts",
    "start": "npm run watch"
  },
  "prettier": {
    "semi": true,
    "arrowParens": "always",
    "printWidth": 110,
    "singleQuote": true,
    "tabWidth": 2
  },
  "author": "David, Matt",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "matter-js": "^0.14.2",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/matter-js": "^0.10.3",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "nodemon": "^1.19.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "short-uuid": "^3.1.1",
    "ts-loader": "^6.2.0",
    "ts-node": "^8.4.1",
    "ts-node-dev": "^1.0.0-pre.43",
    "typescript": "^3.6.3"
  }
}
