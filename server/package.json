{
  "name": "phaser-rts-server",
  "version": "1.0.0",
  "description": "Server side of phaser-rts-game",
  "main": "./src/server.ts",
  "scripts": {
    "tsc": "tsc",
    "runOnly": "ts-node ./src/server.ts",
    "watch": "ts-node-dev --transpileOnly ./src/server.ts",
    "temp": "ts-node ./src/server.ts",
    "debug--not-working": "node --inspect --require ts-node/register ./src/server.ts",
    "start": "npm run watch"
  },
  "prettier": {
    "semi": true,
    "arrowParens": "always",
    "printWidth": 110,
    "singleQuote": true,
    "tabWidth": 2
  },
  "author": "David, Matt",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "jsonwebtoken": "^8.5.1",
    "matter-js": "^0.14.2",
    "mongoose": "^5.9.10",
    "short-uuid": "^3.1.1",
    "socket.io": "^2.3.0",
    "validator": "^13.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/matter-js": "^0.10.9",
    "@types/mongoose": "^5.7.12",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.9.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  }
}
